<%- include('./partials/header.ejs') %>

<% for (const post of posts) { %>
    <img src="<%= post.img %>"/>
    <a href="/catspotting/<%= post._id %>"><h2><%=post.title %></h2></a>
    <h3><%= post.location %></h3>
    <h4>Posted at <%= post.createdAt %></h4>
    <p><%= post.description %></p>
<% } %>

<%- include('./partials/footer.ejs') %>
